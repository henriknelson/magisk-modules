#!/data/perl/bin/bash

bindir="$(dirname "$0")"
execname="$(basename "$0")"

libdir="/data/perl/lib:/data/perl/lib/perl5/5.30.2/aarch64-linux/CORE:/system/lib64:/system/lib"

LD_LIBRARY_PATH="$libdir" LD_PRELOAD="/system/lib64/libm.so /system/lib64/libdl.so /system/lib64/libc.so" /data/perl/bin/perl "$@"
